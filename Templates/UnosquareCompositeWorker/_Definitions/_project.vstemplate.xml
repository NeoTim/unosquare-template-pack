<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Unosquare Composite Worker (.NET Core)</Name>
    <Description>A basic web application that makes it easy to get started with Unosquare Stack.</Description>
    <DefaultName>CompositeWorker</DefaultName>

    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>.NET Core</ProjectSubType>
    <RequiredFrameworkVersion>4.5.2</RequiredFrameworkVersion>
    <CreateInPlace>true</CreateInPlace>
    <PromptForSaveOnCreation>true</PromptForSaveOnCreation>

    <SortOrder>1001</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>

    <Icon>template.ico</Icon>
    <TemplateID>compositeworker.unosquare.templates</TemplateID>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project File="UnosquareCompositeWorker.xproj" ReplaceParameters="true">
      <ProjectItem OpenInEditor="true" TargetFileName="Program.cs">Program.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>